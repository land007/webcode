{
  "name": "webcode-launcher",
  "version": "1.0.1",
  "main": "index.html",
  "window": {
    "title": "webcode",
    "width": 1280,
    "height": 800,
    "min_width": 900,
    "min_height": 580,
    "icon": "assets/icon.png",
    "toolbar": true,
    "show": true
  },
  "chromium-args": "--disable-web-security --allow-running-insecure-content --mixed-context",
  "dependencies": {
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "nw-builder": "^4.17.0"
  },
  "scripts": {
    "build": "node build.js",
    "build:osx-arm64": "node build.js osx arm64",
    "build:osx-x64": "node build.js osx x64",
    "build:win-x64": "node build.js win x64",
    "start": "nw .",
    "start:offline": "./start-offline.sh"
  }
}
